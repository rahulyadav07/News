<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!--
    100    Extra Light or Ultra Light
    200    Light or Thin
    300    Book or Demi
    400    Normal or Regular
    500    Medium
    600    Semi-bold, Demi-bold
    700    Bold
    800    Black, Extra Bold or Heavy
    900    Extra Black, Fat, Poster or Ultra Black
    -->

    <!--400-->
    <font
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/work_sans_regular"
        />

    <!--500-->
    <font
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/work_sans_medium"
        />

    <!--600-->
    <font
        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/work_sans_semi_bold"
        />


    <!--700-->
    <font
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/work_sans_bold"
        />

    <!--800-->
    <font
        app:fontStyle="normal"
        app:fontWeight="800"
        app:font="@font/work_sans_black"
        />



</font-family>